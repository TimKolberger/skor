{
  "name": "cardscore",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "concurrently pnpm:lint:*",
    "lint:eslint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --plugin prettier-plugin-organize-imports --check src test",
    "lint:types": "tsc --noEmit",
    "prebuild": "pnpm lint",
    "prettier:write": "prettier --plugin prettier-plugin-organize-imports --write src test",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/source-code-pro": "5.0.8",
    "@xstate/react": "3.2.2",
    "eslint": "8.50.0",
    "eslint-config-react-app": "7.0.1",
    "framer-motion": "10.16.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-hotkeys-hook": "4.4.1",
    "react-icons": "4.11.0",
    "react-router-dom": "6.16.0",
    "vite-tsconfig-paths": "^4.2.1",
    "xstate": "4.38.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "5.14.9",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-v8": "0.34.5",
    "concurrently": "8.2.1",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-pwa": "0.16.5",
    "vitest": "0.34.5",
    "workbox-window": "^7.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": [
      "prettier --plugin prettier-plugin-organize-imports --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.7.6"
}
