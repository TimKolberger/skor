{
  "name": "cardscore",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "concurrently pnpm:lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --plugin prettier-plugin-organize-imports --check src test",
    "lint:types": "tsc --noEmit",
    "prebuild": "pnpm lint",
    "prettier:write": "prettier --plugin prettier-plugin-organize-imports --write src test",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.4",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/source-code-pro": "5.0.4",
    "@xstate/react": "3.2.2",
    "eslint": "8.44.0",
    "eslint-config-react-app": "7.0.1",
    "framer-motion": "10.12.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-hotkeys-hook": "4.4.1",
    "react-icons": "4.10.1",
    "react-router-dom": "6.14.1",
    "xstate": "4.38.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.7",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "5.14.7",
    "@vitejs/plugin-react": "4.0.2",
    "@vitest/coverage-c8": "0.31.4",
    "concurrently": "8.2.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "typescript": "4.9.5",
    "vite": "4.4.2",
    "vite-plugin-pwa": "0.16.4",
    "vitest": "0.31.4",
    "workbox-window": "^7.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": [
      "prettier --plugin prettier-plugin-organize-imports --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.5.1"
}
